{
	"name": "@rbxts/command",
	"description": "Very simple command/argument management system.",
	"version": "0.0.1",
	"main": "out/init.lua",
	"license": "MIT",
	"scripts": {
		"build": "rbxtsc",
		"watch": "rbxtsc -w",
		"prepublishOnly": "yarn run build"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/zyrakia/rbxts-command"
	},
	"bugs": {
		"url": "https://github.com/zyrakia/rbxts-command/issues"
	},
	"types": "out/index.d.ts",
	"files": [
		"out/**/*.lua",
		"out/**/*.d.ts"
	],
	"publishConfig": {
		"access": "public"
	},
	"keywords": [
		"roblox",
		"roblox-ts",
		"rbxts",
		"command",
		"cmd",
		"arguments",
		"args"
	],
	"dependencies": {
		"@rbxts/zycore": "^2.8.1"
	},
	"devDependencies": {
		"@rbxts/compiler-types": "^1.3.3-types.1",
		"@rbxts/types": "^1.0.588"
	}
}
